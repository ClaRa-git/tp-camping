        <h2>{{title}}</h2>
        <table class="camping-table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Email</th>
                    <th>Role</th>
                    <th>Nom d'utilisateur</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
            {% for user in users %}
                <tr>
                    <td>{{ user.id }}</td>
                    <td>{{ user.email }}</td>
                    <td>{{ user.roles|badge_user }}</td>
                    <td>{{ user.username }}</td>
                    <td>
                        <div class="camping-actions">
                            <a href="{{path('app_user_show', {'id': user.id})}}" class="view-link">Voir</a>
                            <a href="{{path('app_user_edit', {'id': user.id})}}" class="edit-link">Editer</a>
                        </div>                    
                    </td>
                </tr>
            {% else %}
                <tr>
                    <td colspan="4">Pas d'utlisateur trouvÃ©</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
